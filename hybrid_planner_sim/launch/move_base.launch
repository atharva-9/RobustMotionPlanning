<launch>
  <!-- Arguments -->
  <arg name="model" default="$(env TURTLEBOT3_MODEL)" doc="model type [burger, waffle, waffle_pi]"/>
  <arg name="cmd_vel_topic" default="/cmd_vel" />
  <arg name="odom_topic" default="odom" />
  <arg name="move_forward_only" default="false"/>

  <!-- move_base -->
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">

    <rosparam file="$(find hybrid_planner_sim)/config/common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find hybrid_planner_sim)/config/local_costmap_params.yaml" command="load" />
     <param name="base_local_planner" value="hybrid_planner/HybridLocalPlanner" />
<!--      <param name="base_global_planner" value="rrt_star_global_planner/RRTStarPlanner" />
 -->
    <param name="base_global_planner" value="hybrid_planner/HybridGlobalPlanner" />
 
    <rosparam file="$(find hybrid_planner_sim)/config/global_costmap_params.yaml" command="load" />
     <rosparam file="$(find hybrid_planner_sim)/config/common_params.yaml" command="load" ns="global_costmap" />
     <rosparam file="$(find hybrid_planner_sim)/config/hybrid_local_planner_params.yaml" command="load" />
     <rosparam file="$(find hybrid_planner_sim)/config/hybrid_global_planner_params.yaml" command="load" />
  </node>
</launch>
